{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "./public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/routes/**.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/server.js*"
    }
  ]
}
